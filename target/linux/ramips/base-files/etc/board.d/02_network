			"2:lan:2" "3:lan:1" "1:wan" "6t@eth0"
		;;
	xiaomi,mir3p)
		ucidef_add_switch "switch0" \
			"1:lan:3" "2:lan:2" "3:lan:1" "4:wan" "6@eth0"
		;;
	xiaomi,mir4a-100m)
		ucidef_add_switch "switch0" \
