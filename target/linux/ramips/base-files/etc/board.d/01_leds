	;;
hpm)
	ucidef_set_led_netdev "eth" "ETH" "$boardname:green:eth" "eth0"
	set_wifi_led "$boardname:green:wifi"
	;;
ht-tm02)
	ucidef_set_led_netdev "eth" "Ethernet" "$boardname:green:lan" "eth0"
