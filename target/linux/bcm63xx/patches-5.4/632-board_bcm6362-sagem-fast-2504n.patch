 	&board_nb6,
+	&board_fast2504n,
 #endif /* CONFIG_BCM63XX_CPU_6362 */
 };
 
@@ -1984,6 +2016,7 @@ static struct of_device_id const bcm963x
 	{ .compatible = "bt,home-hub-2-a", .data = &board_homehub2a, },
 #endif /* CONFIG_BCM63XX_CPU_6358 */
