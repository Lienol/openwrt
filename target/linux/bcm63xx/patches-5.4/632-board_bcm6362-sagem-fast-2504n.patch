+			[2] = {
+				.used = 1,
+				.phy_id = 3,
+				.name = "Port 3",
+			},
+			[3] = {
+				.used = 1,
+				.phy_id = 4,
+				.name = "Port 4",
+			},
+		},
+	},
+};
 #endif /* CONFIG_BCM63XX_CPU_6362 */
 
 /*
@@ -1893,6 +1924,7 @@ static const struct board_info __initcon
